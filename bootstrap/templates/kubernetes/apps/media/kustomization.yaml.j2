apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
resources:
  - ./namespace.yaml

  {% if qbittorrent.enabled | default(false) %}
  - ./qbittorrent/ks.yaml
  {% endif %}
  {% if plex.enabled | default(false) %}
  - ./plex/ks.yaml
  {% endif %}
