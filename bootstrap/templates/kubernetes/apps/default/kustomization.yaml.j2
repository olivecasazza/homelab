#jinja2: trim_blocks: True, lstrip_blocks: True
---
apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
resources:
  - ./namespace.yaml
  {% if hajimari.enabled | default(false) %}
  - ./hajimari/ks.yaml
  {% endif %}
  {% if qbittorrent.enabled | default(false) %}
  - ./qbittorrent/ks.yaml
  {% endif %}
  {% if jellyfin.enabled | default(false) %}
  - ./jellyfin/ks.yaml
  {% endif %}
  {% if longhorn.enabled | default(false) %}
  - ./longhorn/ks.yaml
  {% endif %}
  {% if discord_template_notifier.enabled | default(false) %}
  - ./discord-template-notifier/ks.yaml
  {% endif %}
